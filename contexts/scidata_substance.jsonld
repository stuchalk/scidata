{
    "@context": [
        {
            "@vocab": "http://www.w3.org/2001/XMLSchema#",
            "dc": "http://purl.org/dc/terms/",
            "sci": "https://stuchalk.github.io/scidata/ontology/scidata.owl#",
            "sub": "https://stuchalk.github.io/scidata/ontology/substance/substance.owl#",
            "substance": {"@id": "sub:substance"},
            "name": {
                "@id": "dc:title",
                "@type": "string"
            },
            "aggregation": {
                "@id": "sub:hasStateOfAggregation",
                "@type": "@id"
            },
            "mixtype": {
                "@id": "sub:hasMixtureType",
                "@type": "@id"
            },
            "phase": {
                "@id": "sub:hasPhase",
                "@type": "@id"
            },
            "constituents": {
                "@id": "sub:hasConstituent",
                "@type": "sci:chemical"
            },
            "properties": {
                "@id": "sub:hasProperty",
                "@type": "sci:parameter"
            }
        }
    ]
}