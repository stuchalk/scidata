{
    "@context": [
        {
            "@vocab": "http://www.w3.org/2001/XMLSchema#",
            "dc": "http://purl.org/dc/terms/",
            "sub": "https://stuchalk.github.io/scidata/ontology/substance/substance.owl#",
            "substance": {"@id": "sub:substance"},
            "name": {
                "@id": "dc:title",
                "@type": "string"
            },
            "aggregation": {
                "@id": "sub:hasStateOfAggregation",
                "@type": "@id"
            },
            "mixtype": {
                "@id": "sub:hasMixtureType",
                "@type": "@id"
            },
            "compound": {
                "@id": "sub:isCompound",
                "@type": "@id"
            },
            "phase": {
                "@id": "sub:hasPhase",
                "@type": "@id"
            },
            "phasetype": {
                "@id": "sub:hasPhaseType",
                "@type": "string"
            },
            "constituents": {"@id": "sub:hasConstituent"},
            "properties": {"@id": "sub:hasProperty"}
        }
    ]
}