{
    "@context": {
        "@vocab": "https://www.w3.org/2001/XMLSchema#",
        "dct": "http://purl.org/dc/terms/",
        "sdo": "https://stuchalk.github.io/scidata/ontology/scidata.owl#",
        "rdfs": "https://www.w3.org/2000/01/rdf-schema#",
        "name": {
            "@id": "dct:title",
            "@type": "string"
        },
        "url": {
            "@id": "dct:identifier",
            "@type": "AnyURI"
        },
        "updated": {
            "@id": "dct:date",
            "@type": "dateTime"
        },
        "source": {
            "@id": "sdo:hasSource",
            "@type": "@id"
        },
        "scope": {
            "@id": "sdo:hasScope",
            "@type": "@id"
        },
        "phase": {
            "@id": "sdo:phaseOfMatter",
            "@type": "string"
        },
        "role": {
            "@id": "sdo:role",
            "@type": "@id"
        },
        "label": {
            "@id": "rdfs:label",
            "@type": "string"
        }
    }
}