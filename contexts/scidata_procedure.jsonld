{
    "@context": {
        "@vocab": "https://www.w3.org/2001/XMLSchema#",
        "dct": "http://purl.org/dc/terms/",
        "sdo": "https://stuchalk.github.io/scidata/ontology/scidata.owl#",
        "proc": "https://stuchalk.github.io/scidata/ontology/scidata_procedure.owl#",
        "procedure": {"@id": "sdo:procedure"},
        "procType": {
            "@id": "proc:procedureType",
            "@type": "string"
        },
        "sequence": {"@id": "proc:procedureSequence"},
        "steps": {"@id": "proc:procedureStep"},
        "step": {
            "@id": "proc:procedureStepNumber",
            "@type": "string"
        },
        "instruction": {
            "@id": "proc:procedureStepInstruction",
            "@type": "string"
        },
        "notes": {
            "@id": "proc:procedureNote",
            "@type": "string"
        }
    }
}