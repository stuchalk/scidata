{
    "@context": [
        {
            "@vocab": "https://www.w3.org/2001/XMLSchema#",
            "cto": "https://stuchalk.github.io/scidata/ontology/chemtwin.owl#",
            "atoms": {"@id": "cto:hasConstituentAtom"},
            "bonds": {"@id": "cto:hasConstituentBond"},
            "elements": {"@id": "cto:containsElement"},
            "atom": {
                "@id": "cto:atom",
                "@type": "@id"
            },
            "bond": {
                "@id": "cto:bond",
                "@type": "@id"
            },
            "charge": {
                "@id": "cto:hasAtomicCharge",
                "@type": "integer"
            },
            "bondedatoms": {
                "@id": "cto:hasBondedAtom",
                "@type": "@id"
            },
            "element": {
                "@id": "cto:element",
                "@type": "@id"
            },
            "elementType": {
                "@id": "cto:isElement",
                "@type": "@id"
            },
            "xcoord": {
                "@id": "cto:hasXcoordinatePosition",
                "@type": "decimal"
            },
            "ycoord": {
                "@id": "cto:hasYcoordinatePosition",
                "@type": "decimal"
            },
            "zcoord": {
                "@id": "cto:hasZcoordinatePosition",
                "@type": "decimal"
            },
            "order": {
                "@id": "cto:bondOrder",
                "@type": "integer"
            },
            "bondcount": {
                "@id": "cto:hasTotalBondCount",
                "@type": "integer"
            },
            "singlebonds": {
                "@id": "cto:hasCovalentSingleBondCount",
                "@type": "integer"
            },
            "doublebonds": {
                "@id": "cto:hasCovalentDoubleBondCount",
                "@type": "integer"
            },
            "triplebonds": {
                "@id": "cto:hasCovalentTripleBondCount",
                "@type": "nonNegativeInteger"
            },
            "aromaticbonds": {
                "@id": "cto:hasAromaticBondCount",
                "@type": "nonNegativeInteger"
            }
        }
    ]
}