              <xs:enumeration value="bold" ncbi:intvalue="10"/>
              <xs:enumeration value="fischer" ncbi:intvalue="11"/>
              <xs:enumeration value="closeContact" ncbi:intvalue="12"/>
              <xs:enumeration value="unknown" ncbi:intvalue="255"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
