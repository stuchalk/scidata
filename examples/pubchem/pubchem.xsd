<xs:element name="Imprint">
  <xs:annotation><xs:documentation>Imprint group</xs:documentation></xs:annotation>
  <xs:complexType>
    <xs:sequence>
      <xs:element name="Imprint_date">
        <xs:annotation><xs:documentation>date of publication</xs:documentation></xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="Date"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Imprint_volume" type="xs:string" minOccurs="0"/>
      <xs:element name="Imprint_issue" type="xs:string" minOccurs="0"/>
      <xs:element name="Imprint_pages" type="xs:string" minOccurs="0"/>
      <xs:element name="Imprint_section" type="xs:string" minOccurs="0"/>
      <xs:element name="Imprint_pub" minOccurs="0">
        <xs:annotation><xs:documentation>publisher, required for book</xs:documentation></xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="Affil"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Imprint_cprt" minOccurs="0">
        <xs:annotation><xs:documentation>copyright date, "    "   "</xs:documentation></xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="Date"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Imprint_part-sup" type="xs:string" minOccurs="0">
        <xs:annotation><xs:documentation>part/sup of volume</xs:documentation></xs:annotation>
      </xs:element>
      <xs:element name="Imprint_language" type="xs:string" default="ENG">
        <xs:annotation><xs:documentation>put here for simplicity</xs:documentation></xs:annotation>
      </xs:element>
      <xs:element name="Imprint_prepub" minOccurs="0">
        <xs:annotation><xs:documentation>for prepublication citations</xs:documentation></xs:annotation>
        <xs:complexType>
          <xs:attribute name="value" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:annotation><xs:documentation>
 submitted	-  submitted, not accepted
 in-press	-  accepted, not published
                </xs:documentation></xs:annotation>
                <xs:enumeration value="submitted"/>
                <xs:enumeration value="in-press"/>
                <xs:enumeration value="other"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
      <xs:element name="Imprint_part-supi" type="xs:string" minOccurs="0">
        <xs:annotation><xs:documentation>part/sup on issue</xs:documentation></xs:annotation>
      </xs:element>
      <xs:element name="Imprint_retract" minOccurs="0">
        <xs:annotation><xs:documentation>retraction info</xs:documentation></xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="CitRetract"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Imprint_pubstatus" minOccurs="0">
        <xs:annotation><xs:documentation>current status of this publication</xs:documentation></xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="PubStatus"/>
          </xs:sequence>
        </xs:complexType>
