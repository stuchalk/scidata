      </xs:element>
      <xs:element name="Date-std_day" type="xs:integer" minOccurs="0">
        <xs:annotation><xs:documentation>day of month (1-31)</xs:documentation></xs:annotation>
      </xs:element>
      <xs:element name="Date-std_season" type="xs:string" minOccurs="0">
        <xs:annotation><xs:documentation>for "spring", "may-june", etc</xs:documentation></xs:annotation>
      </xs:element>
      <xs:element name="Date-std_hour" type="xs:integer" minOccurs="0">
        <xs:annotation><xs:documentation>hour of day (0-23)</xs:documentation></xs:annotation>
      </xs:element>
      <xs:element name="Date-std_minute" type="xs:integer" minOccurs="0">
        <xs:annotation><xs:documentation>minute of hour (0-59)</xs:documentation></xs:annotation>
      </xs:element>
      <xs:element name="Date-std_second" type="xs:integer" minOccurs="0">
        <xs:annotation><xs:documentation>second of minute (0-59)</xs:documentation></xs:annotation>
