                <xs:sequence minOccurs="0" maxOccurs="unbounded">
                  <xs:element name="Auth-list_names_ml_E" type="xs:string"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="Auth-list_names_str">
              <xs:complexType>
                <xs:sequence minOccurs="0" maxOccurs="unbounded">
