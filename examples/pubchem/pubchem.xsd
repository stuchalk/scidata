                <xs:element name="PC-AssayResults_date" minOccurs="0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element ref="Date"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- Assay Readouts/Results for a Tested Substance -->
    <xs:element name="PC-AssayData">
