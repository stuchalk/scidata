          <xs:sequence>
            <xs:element ref="PC-Urn"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="PC-InfoData_value">
        <xs:annotation><xs:documentation>Data Value</xs:documentation></xs:annotation>
        <xs:complexType>
          <xs:choice>
            <xs:element name="PC-InfoData_value_bval">
              <xs:annotation><xs:documentation>Boolean or Binary</xs:documentation></xs:annotation>
              <xs:complexType>
                <xs:attribute name="value" use="required">
                  <xs:simpleType>
                    <xs:restriction base="xs:string">
                      <xs:enumeration value="true"/>
                      <xs:enumeration value="false"/>
                    </xs:restriction>
                  </xs:simpleType>
                </xs:attribute>
              </xs:complexType>
            </xs:element>
            <xs:element name="PC-InfoData_value_bvec">
              <xs:complexType>
                <xs:sequence minOccurs="0" maxOccurs="unbounded">
                  <xs:element name="PC-InfoData_value_bvec_E">
                    <xs:complexType>
                      <xs:attribute name="value" use="required">
                        <xs:simpleType>
                          <xs:restriction base="xs:string">
                            <xs:enumeration value="true"/>
                            <xs:enumeration value="false"/>
                          </xs:restriction>
                        </xs:simpleType>
                      </xs:attribute>
                    </xs:complexType>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="PC-InfoData_value_ival" type="xs:integer">
              <xs:annotation><xs:documentation>Integer (signed or unsigned)</xs:documentation></xs:annotation>
            </xs:element>
            <xs:element name="PC-InfoData_value_ivec">
              <xs:complexType>
                <xs:sequence minOccurs="0" maxOccurs="unbounded">
                  <xs:element name="PC-InfoData_value_ivec_E" type="xs:integer"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="PC-InfoData_value_fval" type="xs:double">
              <xs:annotation><xs:documentation>Float or Double</xs:documentation></xs:annotation>
            </xs:element>
            <xs:element name="PC-InfoData_value_fvec">
              <xs:complexType>
                <xs:sequence minOccurs="0" maxOccurs="unbounded">
                  <xs:element name="PC-InfoData_value_fvec_E" type="xs:double"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="PC-InfoData_value_sval" type="xs:string">
              <xs:annotation><xs:documentation>String</xs:documentation></xs:annotation>
            </xs:element>
            <xs:element name="PC-InfoData_value_slist">
              <xs:complexType>
                <xs:sequence minOccurs="0" maxOccurs="unbounded">
                  <xs:element name="PC-InfoData_value_slist_E" type="xs:string"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="PC-InfoData_value_date">
              <xs:annotation><xs:documentation>Date</xs:documentation></xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element ref="Date"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="PC-InfoData_value_binary" type="xs:hexBinary">
              <xs:annotation><xs:documentation>Binary Data</xs:documentation></xs:annotation>
            </xs:element>
            <xs:element name="PC-InfoData_value_bitlist">
